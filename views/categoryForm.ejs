<%- include('header.ejs') %>

<form action="<%- link %>" method="POST">
    <input type="text" id="name" name="name" value="<%- name %>" placeholder="name">
    <input type="file" id="photo" name="photo" value="<%- photo %>" accept="image/*">
    <input type="text" id="product" name="products" value="<%- products %>" placeholder="Product Id">
    <input type="submit" placeholder="click to submit">
</form>